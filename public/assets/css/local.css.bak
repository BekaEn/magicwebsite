/*-- -------------------------- -->
<---        Navigation          -->
/* Desktop - 1024px */
@media only screen and (min-width: 64rem) {
  #cs-navigation {
    box-shadow: 0px 2px 10px rgba(0, 0, 0, 0.1);
  }
  
  #cs-navigation .cs-li-link {
    position: relative;
    text-transform: uppercase;
    font-weight: 500;
    font-size: 0.9rem;
    letter-spacing: 0.05em;
  }
  
  #cs-navigation .cs-li-link:before {
    height: 3px;
    border-radius: 2px;
    background: var(--primary);
    opacity: 1;
  }
  
  #cs-navigation .cs-drop-ul {
    background-color: #fff;
    border-radius: 0 0 4px 4px;
    box-shadow: 0px 8px 20px rgba(0, 0, 0, 0.1);
    border-top: 3px solid var(--primary);
    padding: 0.5rem 0;
  }
  
  #cs-navigation .cs-drop-li {
    padding: 0;
  }
  
  #cs-navigation .cs-drop-li .cs-li-link {
    text-transform: none;
    letter-spacing: 0;
    font-weight: normal;
    padding: 0.75rem 1.5rem;
    transition: all 0.3s ease;
  }
  
  #cs-navigation .cs-drop-li .cs-li-link:hover {
    background-color: rgba(0, 0, 0, 0.03);
    color: var(--primary);
  }
  
  #cs-navigation .cs-drop-li .cs-li-link:before {
    display: none;
  }
  
  #cs-navigation .cs-button-solid {
    border-radius: 4px;
    box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);
    transition: all 0.3s ease;
  }
  
  #cs-navigation .cs-button-solid:hover {
    transform: translateY(-2px);
    box-shadow: 0px 6px 15px rgba(0, 0, 0, 0.15);
  }
}

/*-- -------------------------- -->
<---      Our Approach          -->
<--- -------------------------- -*/
/* Mobile - 360px */
@media only screen and (min-width: 0rem) {
  .our-approach-sec {
    background-color: #f8f9fa;
    position: relative;
    overflow: hidden;
    padding: var(--sectionPadding);
  }
  
  .our-approach-sec:before {
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    background-color: var(--primary);
    opacity: 0.05;
    width: 300px;
    height: 300px;
    border-radius: 50%;
    transform: translate(100px, -150px);
  }
  
  .our-approach-sec:after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 0;
    background-color: var(--secondary);
    opacity: 0.05;
    width: 250px;
    height: 250px;
    border-radius: 50%;
    transform: translate(-100px, 100px);
  }
  
  .our-approach-sec .cs-title {
    margin-bottom: 2rem;
  }
  
  .our-approach-sec .cs-text {
    margin-bottom: 2.5rem;
  }
  
  .what-expect-list {
    list-style: none;
    padding: 0;
    margin: 0;
  }
  
  .what-expect-list .cs-li-text {
    position: relative;
    padding-left: 2rem;
    margin-bottom: 1rem;
    font-size: 1.1rem;
  }
  
  .what-expect-list .cs-li-text:before {
    content: '✓';
    position: absolute;
    left: 0;
    top: 0;
    color: var(--primary);
    font-weight: bold;
    font-size: 1.2rem;
  }
  
  .cs-image-group .cs-picture {
    border-radius: 8px;
    overflow: hidden;
    box-shadow: var(--standardShadow);
    transition: all 0.3s ease;
  }
  
  .cs-image-group .cs-picture:hover {
    transform: translateY(-5px);
    box-shadow: var(--hoverShadow);
  }
}

/*-- -------------------------- -->
<---       Media Queries        -->
<--- -------------------------- -*/
/* Tablet - 768px */
@media only screen and (min-width: 48rem) {
  #reviews .cs-card-group {
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
    gap: 1.5rem;
  }
  
  #reviews .cs-item {
    flex: 1;
    margin: 0;
  }
}

