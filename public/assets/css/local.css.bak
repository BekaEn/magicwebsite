/*-- -------------------------- -->
<---           Reviews          -->
<--- -------------------------- -*/
/* Mobile - 360px */
@media only screen and (min-width: 0rem) {
  #reviews {
    background-color: #f8f9fa;
    position: relative;
    padding: var(--sectionPadding);
    overflow: hidden;
  }
  
  #reviews:before {
    content: '';
    position: absolute;
    top: 50%;
    right: 0;
    background-color: var(--primary);
    opacity: 0.03;
    width: 400px;
    height: 400px;
    border-radius: 50%;
    transform: translate(200px, -200px);
    z-index: 1;
  }
  
  #reviews .cs-container {
    position: relative;
    z-index: 10;
  }
  
  #reviews .cs-title {
    text-align: center;
    margin-bottom: 3rem;
    max-width: 100%;
  }
  
  #reviews .cs-topper {
    text-align: center;
    display: block;
    margin-left: auto;
    margin-right: auto;
    margin-bottom: 0.5rem;
  }
  
  #reviews .cs-card-group {
    gap: 1.5rem;
  }
  
  #reviews .cs-item {
    background-color: #fff;
    padding: 2rem;
    border-radius: 8px;
    box-shadow: var(--standardShadow);
    transition: all 0.3s ease;
  }
  
  #reviews .cs-item:hover {
    transform: translateY(-5px);
    box-shadow: var(--hoverShadow);
  }
  
  #reviews .cs-item-p {
    position: relative;
    padding-left: 1.5rem;
    font-style: italic;
    margin-bottom: 1.5rem;
  }
  
  #reviews .cs-item-p:before {
    content: """;
    position: absolute;
    left: 0;
    top: 0;
    font-size: 2.5rem;
    line-height: 1;
    color: var(--primary);
    font-weight: bold;
    opacity: 0.5;
  }
  
  #reviews .cs-reviewer {
    font-weight: bold;
    color: var(--headerColor);
    display: flex;
    align-items: center;
    gap: 0.5rem;
  }
  
  #reviews .cs-reviewer:before {
    content: '';
    display: block;
    width: 3rem;
    height: 2px;
    background-color: var(--primary);
    opacity: 0.5;
  }
  
  #reviews .cs-desc {
    color: var(--secondaryLight);
    font-weight: normal;
  }
}

